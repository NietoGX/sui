[
  {
    "test": ".tsx?$",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": true,
        "decorators": false,
        "dynamicImport": true
      },
      "transform": {
        "legacyDecorator": true,
        "react": {
          "useBuiltins": true,
          "runtime": "automatic"
        }
      },
      "target": "es5",
      "loose": true,
      "externalHelpers": true
    },
    "env": {
      "targets": {
        "ie": "11"
      },
      "dynamicImport": true,
      "loose": true,
      "mode": "entry",
      "coreJs": 3
    }
  },
  {
    "test": ".jsx?$",
    "jsc": {
      "parser": {
        "syntax": "ecmascript",
        "jsx": true,
        "decorators": false,
        "dynamicImport": true
      },
      "transform": {
        "legacyDecorator": true,
        "react": {
          "useBuiltins": true,
          "runtime": "automatic"
        }
      },
      "target": "es5",
      "loose": true,
      "externalHelpers": true
    },
    "env": {
      "targets": {
        "ie": "11"
      },
      "dynamicImport": true,
      "loose": true,
      "mode": "entry",
      "coreJs": 3
    }
  }
]
